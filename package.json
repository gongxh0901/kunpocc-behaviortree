        "registry": "https://registry.npmjs.org/"
    },
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "@types/lodash": "^4.17.13",
        "@types/node": "^22.10.2",
        "rollup": "^4.28.1",
        "rollup-plugin-dts": "^6.1.1",
        "ts-node": "^10.9.2",
        "tslib": "^2.6.2"
    }
}